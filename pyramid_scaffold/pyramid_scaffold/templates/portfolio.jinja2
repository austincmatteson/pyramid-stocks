{% extends "base.jinja2" %} 

{% block content %}
<ul>
  {% for entry in entries %}
  <li>
    <h3>{{entry.companyName}}</h3>
    <h3>{{entry.symbol}}</h3>
    <p>
      <a href="{{request.route_url('detail', symbol=entry.symbol)}}">view more</a>
    </p>
  </li>
  {% endfor %}
</ul>
{% endblock content %}
